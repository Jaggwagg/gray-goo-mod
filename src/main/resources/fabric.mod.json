{
  "schemaVersion": 1,
  "id": "gray_goo",
  "version": "${version}",
  "name": "Gray Goo",
  "description": "A mod which adds destructive nanotechnology into Minecraft!",
  "authors": [
    "Jaggwagg"
  ],
  "contact": {
    "homepage": "https://github.com/Jaggwagg/gray-goo-mod",
    "sources": "https://github.com/Jaggwagg/gray-goo-mod.git",
    "issues": "https://github.com/Jaggwagg/gray-goo-mod/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/gray_goo/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "jaggwagg.gray_goo.GrayGoo"
    ],
    "client": [
      "jaggwagg.gray_goo.client.GrayGooClient"
    ]
  },
  "mixins": [
    "gray_goo.mixins.json",
    {
      "config": "gray_goo.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
